---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Hackety Hack</title>
 <link href="http://steveklabnik.github.com/hacketyhack/atom.xml" rel="self"/>
 <link href="http://hackety-hack.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://steveklabnik.github.com/hacketyhack</id>
 <author>
   <name>Steve Klabnik</name>
   <email>steve@steveklabnik.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://steveklabnik.github.com/hacketyhack{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://steveklabnik.githu.com/hacketyhack{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
